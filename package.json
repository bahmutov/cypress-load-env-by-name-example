{
  "name": "cypress-load-env-by-name-example",
  "version": "1.0.0",
  "description": "Load Cypress config and env variables from separate JSON files",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "start": "serve public",
    "start:staging": "serve public --ssl-cert ./.cert/staging-cert.pem --ssl-key ./.cert/staging-key.pem",
